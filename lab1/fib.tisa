0   // адрес 0
1   // адрес 4
-1  // адрес 8   (для вычитания n = n - 1)
LD   X10, 0(X9)        ; X10 = 0  ; 0b 011010 01001 01010 0000000000000000 // адрес 12
LD   X11, 4(X9)        ; X11 = 1  ; 0b 011010 01001 01011 0000000000000100 // адрес 16
LD   X12, 8(X9)        ; X12 = -1 ; 0b 011010 01001 01100 0000000000001000 // адрес 20

SLTI X15, X0, 2        ; X15 = (n < 2) ? 1 : 0 ; 0b 010110 01111 00000 0000000000000010 // адрес 24
BNE  X15, X10, base    ; если X15 != 0 → base_case ; 0b 100100 01111 01010 0000000000001000 // адрес 28

        ; a = 0, b = 1
ADD  X13, X10, X10     ; a = 0 ; 0b 000000 01010 01010 01101 00000 101011 // адрес 32
ADD  X14, X11, X10     ; b = 1 ; 0b 000000 01011 01010 01110 00000 101011 // адрес 36

loop:
    ADD  X15, X13, X14     ; c = a + b ; 0b 000000 01101 01110 01111 00000 101011 // адрес 40
    ADD  X13, X14, X10     ; a = b ;     0b 000000 01110 01010 01101 00000 101011 // адрес 44
    ADD  X14, X15, X10     ; b = c ;     0b 000000 01111 01010 01110 00000 101011 // адрес 48
    ADD  X0,  X0,  X12     ; n = n - 1   0b 000000 00000 01100 00000 00000 101011 // адрес 52
    BNE  X0,  X11, loop    ; пока n != 1 — повторять ; 0b 100100 00000 01011 1111111111111011 // адрес 56

    ; результат = b
    ADD  X0,  X14, X10     ; X0 = b ; 0b 000000 01110 01010 00000 00000 101011 // адрес 60 
base:
    LD X8, 4(X9) ; 0b 011010 01001 01000 0000000000000100 // адрес 64
    SYSCALL               ; X8 = SIGPRINT, X0 = результат ; 0b 000000 00000000000000000000 111110 // адрес 68
